    <!-- <motor joint="right_gripper_tip_joint_right" gear="-100"/> -->

    
  </actuator>

  <contact>
    <exclude body1="right_gripper_link_left" body2="left_gripper_link_left"/>
    <exclude body1="right_gripper_tip_left" body2="left_gripper_tip_left"/>

    <exclude body1="right_gripper_link_right" body2="left_gripper_link_right"/>
    <exclude body1="right_gripper_tip_right" body2="left_gripper_tip_right"/>
  </contact>

