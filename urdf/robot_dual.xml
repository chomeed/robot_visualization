  <option>
        <flag multiccd="enable"/>
  </option>


  <actuator>
    <!-- Left Arm -->
    <motor name="link01_left" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_left_arm_1"/>
    <motor name="link02_left" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_left_arm_2"/>
    <motor name="link03_left" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_left_arm_3"/>
