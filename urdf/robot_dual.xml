    <motor name="link06_left" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_left_arm_6"/>

    <motor name="gripper_motor_left_left" ctrllimited="true" ctrlrange="-1 1" gear="100" joint="left_gripper_link_joint_left"/>
    <!-- <motor joint="left_gripper_tip_joint_left" gear="100"/> -->
    <!-- <motor name="gripper_motor_right_left" ctrllimited="true" ctrlrange="-1 1" gear="-100" joint="right_gripper_link_joint_left"/> -->
    <!-- <motor joint="right_gripper_tip_joint_left" gear="-100"/> -->


    <!-- Right Arm -->
    <motor name="link01_right" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_right_arm_1"/>
    <motor name="link02_right" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_right_arm_2"/>
    <motor name="link03_right" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_right_arm_3"/>
    <motor name="link04_right" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_right_arm_4"/>
    <motor name="link05_right" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_right_arm_5"/>
    <motor name="link06_right" ctrllimited="true" ctrlrange="-0.5 0.5" gear="50" joint="joint_right_arm_6"/>

    <motor name="gripper_motor_left_right" ctrllimited="true" ctrlrange="-1 1" gear="100" joint="left_gripper_link_joint_right"/>
    <!-- <motor joint="left_gripper_tip_joint_right" gear="100"/> -->
