    <!-- Left Arm -->
    <joint joint1="left_gripper_link_joint_left" joint2="right_gripper_link_joint_left" polycoef="0 -1 0"/>
    <weld body1="rh_p12_rn_base_left" body2="right_gripper_tip_left" solref="0.02 1" solimp="0.95 0.95 0.001 0.5 6"/>
